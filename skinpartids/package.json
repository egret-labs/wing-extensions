{
	"name": "SkinPartIdsTool",
	"description": "Text replacement functions e.g. change case, reverse and ASCII Art.",
	"version": "0.1.0",
	"publisher": "egret",
	"categories":[
		"Other"
	],
	"icon": "images/SkinPartIdsToolIcon.png",
	"bugs": {
		"url": "https://github.com/egret-labs/wing-extensions/issues"
	},
	"homepage": "https://github.com/egret-labs/wing-extensions/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/egret-labs/wing-extensions.git"
	},
	"activationEvents": [
		"*"
	],
	"engines": {
		"wing": "^2.5.0"
	},
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.copyIds",
				"title": "复制EXML中的皮肤部件"
			}
		],
		"keybindings": [
			{
				"command": "extension.copyIds",
				"key": "Alt+C"
			}
		]
	},
	"scripts": {
		"compile": "tsc -watch"
	},
	"dependencies": {
        "copy-paste": "^1.1.3"
	},
	"devDependencies": {
	}
}